extends base.pug 

block scripts 
 script(sre='/static/index.js')

block style 
    style
       include ../static/style.css 
       include ../static/styleContact.css 

block content 
    div.container 
        h1 Contact Us
        form(action="/contact", method="post" , class="MyForm")
            input(type="text", class="myInput", name="Name", placeholder="Enter Your Name")
            input(type="number", class="myInput", name="Phone", placeholder="Enter Your Contact Number")
            input(type="email", class="myInput", name="eMail", placeholder="Enter Your E-mail")
            input(type="text", class="myInput", name="Address", placeholder="Enter Your Address")
            input(type="text", class="myInput", name="Name", placeholder="Enter Your Name")
            button(class="btn")
                | Submit 
